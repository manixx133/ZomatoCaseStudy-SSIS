              SQLTask:SqlStatementSource="declare @name nvarchar(1000);&#xA;SET @name = ?;&#xA;declare @url nvarchar(1000);&#xA;SET @url = ?;&#xA;WITH CTE1 AS&#xA;(&#xA;SELECT A.*,B.[CUSINE_ID]&#xA;FROM [stage].[tbl_csv] A&#xA;LEFT JOIN&#xA;[data].[tbl_cusine_cat] B&#xA;ON A.CUSINE_CATEGORY = B.CUSINE_CATEGORY&#xA;WHERE A.NAME =@name AND A.URL=@url&#xA;),&#xA;CTE2 AS&#xA;(&#xA;SELECT A.[NAME],A.[PRICE],A.[CUSINE_ID],B.[CITY_ID],A.[PAGE_NO],A.[CUSINE_TYPE],A.[TIMING],A.[RATING_TYPE],A.[RATING],A.[VOTES],A.[URL] FROM CTE1 A&#xA;LEFT JOIN&#xA;[data].[tbl_cityregion] B&#xA;ON A.CITY = B.CITY AND A.REGION=B.REGION&#xA;)&#xA;INSERT INTO [data].[tbl_restaurant]&#xA;SELECT * FROM CTE2" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
